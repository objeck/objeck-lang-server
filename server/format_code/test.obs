use System.IO.File;
use Collection.Generic;

class Test {
	function : Main(args : String[]) ~ Nil {
		if(args->Size() = 1) {
			source := FileReader->ReadFile(args[0]);
			if(source <> Nil) {
				options := Map->New()<String, String>;
				options->Insert("function-space", "true");

				CodeFormatter->Format(source, options)->PrintLine();
			};
		};
	}
}