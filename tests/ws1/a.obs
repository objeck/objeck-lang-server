class Test {
	@abc : ValueHolder;

	New() {
		@abc := ValueHolder->New(7);
	}

	function : Main(args : String[]) ~ Nil {
		vh : ValueHolder := ValueHolder->New(13);
		vh->GetValue()->PrintLine();
		
		DoIt(7);

		t := Test->New();
		t->Bar()->PrintLine();
	}

	method : Foo() ~ Int {
		return 7 + @abc->GetValue();
	}

	method : Bar() ~ Int {
		return @abc + @abc;
	}
	
	function : DoIt(i : Int) ~ Int {
		return i * 13;
	}
}